{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "@vue/typescript",
    "@vue/prettier"
  ],
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "no-unused-vars": "error",
    "no-multiple-empty-lines": "error",
    "curly": [
      2,
      "all"
    ]
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  }
}
